<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.esiea.praddaude.aura">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:configChanges="locale|orientation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SettingsActivity"
            android:configChanges="locale|orientation" />
        <activity android:name=".ProjetActivity" />
        <activity android:name=".AvancementActivity" />
        <activity android:name=".MediasActivity" />
        <activity android:name=".CessnaActivity" />
        <activity android:name=".MoteurActivity" />
        <activity android:name=".InstruActivity" />
        <activity android:name=".RadioActivity" />
        <activity android:name=".ChecklistActivity" />

        <service
            android:name=".GetNewsServices"
            android:exported="false"></service>
    </application>

</manifest>